<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_Future_Date</fullName>
    <active>true</active>
    <errorConditionFormula>TODAY()  -  Enrollment_Date__c &lt;0 ||
   ISBLANK( Enrollment_Date__c )</errorConditionFormula>
    <errorDisplayField>Enrollment_Date__c</errorDisplayField>
    <errorMessage>No Future Date or Is Not Blank</errorMessage>
</ValidationRule>
