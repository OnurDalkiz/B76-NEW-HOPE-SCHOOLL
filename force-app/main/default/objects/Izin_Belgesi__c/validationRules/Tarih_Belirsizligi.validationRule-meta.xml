<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tarih_Belirsizligi</fullName>
    <active>true</active>
    <errorConditionFormula>OR( Izinden_Donus_Tarihi__c  &lt;= Izine_Cikis_Tarihi__c  , Rapor_Baslangic_Tarihi__c &gt;= Rapor_Biti_Tarihi__c )</errorConditionFormula>
    <errorMessage>Başlangıç tarihi bitiş tarihinden büyük olamaz</errorMessage>
</ValidationRule>
