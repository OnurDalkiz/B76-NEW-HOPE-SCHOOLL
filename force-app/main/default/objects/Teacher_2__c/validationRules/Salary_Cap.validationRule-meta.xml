<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Salary_Cap</fullName>
    <active>true</active>
    <errorConditionFormula>Salary__c &lt;3000 ||  Salary__c &gt;10000</errorConditionFormula>
    <errorDisplayField>Salary__c</errorDisplayField>
    <errorMessage>Teacher Salary Must Be &quot;3000-10000&quot;</errorMessage>
</ValidationRule>
